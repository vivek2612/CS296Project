unset log
reset
set autoscale
set title "Average Step Time vs Rerun Value"
set ylabel "Frequency of Rerun Values"
set xlabel "StepTime(ms)"
set y2label "Cumulative frequency"
set ytics nomirror
set y2tics 10 textcolor rgbcolor "blue"
set key left box linewidth 1.0
set datafile separator ','
bw = 0.03
set boxwidth bw*0.8
bin(x,width)=width*floor(x/width)
set macro
set style fill solid 1.0 noborder
 set term png
set output "../plots/g14_plot05.png
plot "../data/Temp5.csv" using (bin($1,bw)+bw/2): (2.0)  smooth frequency with boxes title "Freq", \
'' using ($1):(1.0) smooth cumulative axes x1y2 with lines title "Cum Freq"




















